
kill(all)$
load("helper_functions.mac")$

f: 4*x^5+x^2+x$
tests: [
    range(1,5) = [1,2,3,4,5],
    member(rand_sign(1,2), [1,2,-1,-2]),
    
    coeffs(f,x,5) = 4,
    coeffs(f,x,3) = 0,
    coeffs(f,x,2) = 1,
    
    lead_coeff(f,x) = 4
]$
